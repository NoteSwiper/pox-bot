{
	"version": 1,
	"disable_existing_loggers": false,
	"formatters": {
		"default": {
			"format": "[{asctime}] [{levelname:<8}] {name}: {message}",
			"datefmt": "%Y-%m-%d %H:%M:%S",
			"style": "{"
		},
		"detailed": {
			"format": "[{asctime}] [{levelname:<8}] {name}: {message}",
			"datefmt": "%Y-%m-%d %H:%M:%S",
			"style": "{"
		},
		"forchat": {
			"format": "[{asctime}] {message}",
			"datefmt": "%Y-%m-%d %H:%M:%S",
			"style": "{"
		}
	},
	"handlers": {
		"console": {
			"class": "logging.StreamHandler",
			"level": "DEBUG",
			"formatter": "default",
			"stream": "ext://sys.stdout"
		},
		"file": {
			"class": "logging.handlers.RotatingFileHandler",
			"level": "DEBUG",
			"formatter": "default",
			"filename": "logs/discord.log",
			"maxBytes": 33554432,
			"backupCount": 128,
			"encoding": "utf-8"
		},
		"file_interact": {
			"class": "logging.FileHandler",
			"level": "INFO",
			"formatter": "forchat",
			"filename": "logs/interactions.log",
			"encoding": "utf-8"
		},
		"console_interact": {
			"class": "logging.StreamHandler",
			"level": "DEBUG",
			"formatter": "forchat",
			"stream": "ext://sys.stdout"
		}
	},
	"loggers": {
		"__main__": {
			"level": "DEBUG",
			"handlers": ["file"],
			"propagate": false
		},
		"bot": {
			"level": "DEBUG",
			"handlers": ["file"],
			"propagate": false
		},
		"discord": {
			"level": "INFO",
			"handlers": ["file"],
			"propagate": false
		},
		"stuff": {
			"level": "DEBUG",
			"handlers": ["file"],
			"propagate": false
		},
		"interactions": {
			"level": "INFO",
			"propagate": false,
			"handlers": ["file_interact","console_interact"]
		}
	}
}